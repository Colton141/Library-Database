<h2>Enter New Patron Information:</h2>

<form asp-controller="patron" asp-action="create" method="post">
  <div class="">
    <label for="patronName">Patron Name</label>
    <input type="text" name="patronName">
  </div>
  <div class="">
    <label for="checkoutDate">Date Enrolled:</label>
    <input type="text" name="checkoutDate">
  </div>

  <button type="submit">Save Patron</button>
</form>
